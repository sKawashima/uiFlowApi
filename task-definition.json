{
    "family": "uiflow",
    "taskRoleArn": "ECS-task-full-access",
    "executionRoleArn": "arn:aws:iam::${AWS::AccountId}:role/ecsTaskExecutionRole",
    "containerDefinitions": [{
        "name": "uiflow-api",
        "image": ".",
        "cpu": 10,
        "memory": 500,
        "portMappings": [{
            "containerPort": 80,
            "hostPort": 80,
            "protocol": "tcp"
        }],
        "essential": true
    }],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512"
}
